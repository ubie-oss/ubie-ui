{
  "name": "@ubie/ubie-ui",
  "version": "0.0.5",
  "description": "React components for creating Ubie applications.",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "test": "run-p test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test -c playwright.config.ts",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx ./src --max-warnings=0",
    "lint:prettier": "prettier --check src",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "lint:markuplint": "markuplint \"./src/**/*.tsx\" -p",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier -w src",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "generate": "npm run generate:scaffold && npm run generate:barrel",
    "generate:scaffold": "scaffdog generate component",
    "generate:barrel": "node ./scripts/barrel.mjs",
    "prestorybook": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubie-oss/ubie-ui.git"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.3",
    "@babel/core": "7.22.11",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@headlessui/react": "^1.7.10",
    "@markuplint/jsx-parser": "^4.4.0",
    "@markuplint/react-spec": "^4.2.0",
    "@playwright/experimental-ct-react": "^1.38.1",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "11.1.3",
    "@storybook/addon-actions": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@types/node": "18.17.12",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@ubie/design-tokens": "0.1.4",
    "@ubie/prettier-config": "0.1.0",
    "@ubie/ubie-icons": "^0.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "10.4.15",
    "babel-loader": "9.1.3",
    "chalk": "5.3.0",
    "clsx": "2.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "execa": "8.0.1",
    "glob": "10.3.3",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "markuplint": "^4.4.0",
    "npm-run-all": "4.1.5",
    "playwright": "^1.38.1",
    "prettier": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.28.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "4.0.2",
    "scaffdog": "3.0.0",
    "storybook": "^8.0.5",
    "style-loader": "3.3.3",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "tsup": "^7.2.0",
    "typescript": "5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "@headlessui/react": ">1.7.0",
    "@ubie/design-tokens": "^0.0.10 || ^0.1.0",
    "clsx": ">1.2.0",
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "author": "Ubie, Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ubie-oss/ubie-ui/issues"
  },
  "homepage": "https://github.com/ubie-oss/ubie-ui#readme"
}
